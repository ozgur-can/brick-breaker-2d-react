{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": [ "error", "never" ],
        "react/function-component-definition": "off",
        "import/no-cycle": "off",
        "import/no-unresolved": "off",
        "react/jsx-props-no-spreading": "off",
        "react/prop-types": "off"
    }
}
